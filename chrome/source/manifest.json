{
  "name": "Live CSS Editor",
  "version": "1.0",
  "background_page": "bg.html",
  "options_page": "options.html",
  "description": "Live Write CSS onto any page",
  "permissions": [
    "tabs", "<all_urls>"
  ],
  "content_scripts": [ {
    "matches": ["<all_urls>"],
    "js": ["inject.js"]
  }],
  "browser_action": {
    "default_icon": "icon-color.png",
    "name": "Enable the editor"
  },
  "icons": { 
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}